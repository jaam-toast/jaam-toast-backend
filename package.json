{
  "name": "jaam-toast-backend",
  "version": "1.9.0",
  "private": true,
  "engines": {
    "node": ">=16.15.0 <=18.12.0"
  },
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src/app.ts",
    "dev": "concurrently \"yarn server-main\" \"yarn server-build\"",
    "server-main": "ts-node-dev --transpile-only --respawn -r tsconfig-paths/register --watch ./src --trace-warnings --trace-uncaught -- ./src/app.ts",
    "server-build": "ts-node-dev --transpile-only --respawn -r tsconfig-paths/register --watch ./src --trace-warnings --trace-uncaught -- ./src-build/app.ts",
    "watch-ts": "npx tsc --watch",
    "lint": "eslint --ext .ts src/",
    "test": "node --experimental-vm-modules --trace-warnings node_modules/jest/bin/jest.js --verbose --detectOpenHandles"
  },
  "dependencies": {
    "axios": "^1.1.3",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "dotenv": "^16.0.3",
    "express": "~4.16.1",
    "helmet": "^6.0.0",
    "http-errors": "^2.0.0",
    "joi": "^17.6.3",
    "joigoose": "^8.0.2",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^5.1.0",
    "mongoose": "^6.6.5",
    "morgan": "^1.10.0",
    "nanoid": "^4.0.2",
    "socket.io": "^4.5.3"
  },
  "devDependencies": {
    "@jest/globals": "^29.3.1",
    "@octokit/types": "^8.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.14",
    "@types/http-errors": "^1.8.2",
    "@types/jest": "^29.2.3",
    "@types/joigoose": "^2.0.7",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/mongodb": "^4.0.7",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.8.5",
    "@types/ssh2": "^1.11.8",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "eslint": "^8.25.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "prettier": "^2.7.1",
    "superagent": "^8.0.4",
    "supertest": "^6.3.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.8.4"
  },
  "packageManager": "yarn@1.22.19"
}
